{
  "cli": {
    "version": ">= 9.0.8"
  },
  "build": {
    "development": {
      "env": {
        "APP_VARIANT": "development"
      },
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true
      }
    },
    "production:pretend" : {
      "channel": "production",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
